projects:LOD2 a foaf:Project, aksw:FundedProject;
    foaf:homepage <http://lod2.eu>;
    aksw:hookline "Creating Knowledge out of Interlinked Data";
    dcterms:abstract "The goal of the LOD2 project is to develop infrastructure technology and best practices that fill the chasm between structured-linked-data and applied model logic & reasoning, en route redefining the Web as we know it. In doing so, LOD2 will integrate and syndicate linked data with large-scale existing applications and showcase the benefits in three application scenarios including Media & Publishing, Corporate Data Intranets and eGovernment.";
    site:content """
* Partners: Centrum Wiskunde & Informatica (NL), National University of Ireland, Galway (IRL), Freie Universität Berlin (D), OpenLink Software (GB), Semantic Web Company (A), TenForce (B), Exalead (F), Wolters Kluwer Deutschland (D), Open Knowledge Foundation (GB)
* Duration: 2010–2014
* Funding Programme: FP7-ICT-2009–5 (Collaborative Project)
"""^^sysont:Markdown;
    ov:screenshot <http://lh5.ggpht.com/-qFsSKX-1_dw/Tpx24oHmtEI/AAAAAAAAA9g/XOeQQWjBzCc/s800/lod2demo.png>;
    sioc:feed <http://lod2.eu/BlogPost/feed>;
    doap:maintainer people:SoerenAuer;
    aksw:publicationTag "lod2page";
    aksw:publicationStyle "extended";
    rdfs:label "LOD2".

projects:LATC a foaf:Project, aksw:FundedProject;
    foaf:logo <http://latc-project.eu/sites/default/files/latc-logo.gif>;
    foaf:homepage <http://latc-project.eu>;
    aksw:hookline "LOD Around-the-Clock";
    dcterms:abstract "This EU-funded support action aims at making it easier for data owners to publish Linked Data and interlink it with other data sources. A user-friendly 24/7 interlinking engine is developed within the project, allowing to compute and update interlinks as well as to assess their quality.";
    site:content """
* Partners: National University of Ireland, Galway (IRL), Vrije Universiteit Amsterdam (NL), Freie Universität Berlin (D), Talis Ltd. (GB)
* Duration: 2010–2012
* Funding Programme: FP7-ICT-2009.4.3 (Support Action)
"""^^sysont:Markdown;
    doap:maintainer people:JensLehmann;
    sioc:feed <http://blog.aksw.org/category/projects/latc/feed/>;
    rdfs:label "LATC".

projects:SCMS a foaf:Project, aksw:FundedProject;
    foaf:logo <http://www.scms.eu/wp-content/themes/scms-theme/images/scms-logo.png>;
    foaf:homepage <http://scms.eu>;
    aksw:hookline "Semantic Content Management Systems";
    dcterms:abstract "SCMS aims at combining automatic knowledge discovery and participative, end-user-driven knowledge curation and extension techniques through Social Semantic Web approaches so as to create novel applications in Enterprise Knowledge Management and real-time News Mining.";
    site:content """
* Partners: Punk.Netservices GmbH (A), OpenLink Software Ltd. (UK), Netresearch GmbH & Co. KG (D), Digital Trowel Inc. (IL)
* Duration: 2010–2012
* Funding Programme: Eurostars
"""^^sysont:Markdown;
    doap:maintainer people:AxelNgonga;
    sioc:feed <http://www.scms.eu/feed/>;
    rdfs:label "SCMS".

projects:LE4SW a foaf:Project, aksw:FundedProject;
    foaf:homepage <http://le4sw.de>;
    aksw:status "finished";
    aksw:hookline "Regional Technology Platform of Social Semantic Collaboration";
    dcterms:abstract "Aim of the project is to (further) develop AKSW's semantic data wiki technology platform by integrating view maintenance, query subsumption and schema evolution algorithms for the RDF data model and adopt it towards the requirements of business information systems and Web content management.";
    site:content """
* Partners: Ebrosia GmbH, Business Intelligence GmbH (D), Netresearch GmbH & Co. KG (D)
* Duration: 2009–2011
* Funding Programme: WK Potential, German Ministry of Education and Research (BMBF)
"""^^sysont:Markdown;
    doap:maintainer people:SoerenAuer;
    rdfs:label "LE4SW".

projects:SoftWiki a foaf:Project, aksw:FundedProject;
    foaf:logo <http://lh3.ggpht.com/-O0rHt6DYrHE/TlzV1zEb4sI/AAAAAAAAApo/bg7W55zdiIQ/s800/logo_softwiki-200px.png>;
    foaf:homepage <http://softwiki.de>;
    aksw:status "finished";
    aksw:hookline "Semantics- and Community-Based Requirements Engineering";
    dcterms:abstract "The project delivers fundamental building blocks for early phases of Requirements Engineering by establishing a stakeholder-driven knowledge engineering process for requirements elicitation, organisation and management.";
    site:content """
* Partners: Universität Duisburg-Essen, T-Systems MMS, ProDV AG, LeCoS GmbH, QA Systems GmbH, ISA Tools GmbH
* Duration: 2006–2009
* Funding Programme: [research initiative “Software Engineering 2006”](http://www.softwarefoerderung.de/), German Ministry of Education and Research (BMBF)
"""^^sysont:Markdown;
    sioc:feed <http://softwiki.de/netzwerk/feed/>;
    doap:maintainer people:ThomasRiechert;
    rdfs:label "SoftWiki".

projects:OntoWikiEU a foaf:Project, aksw:FundedProject;
    site:content "..."^^sysont:Markdown;
    foaf:homepage <http://ontowiki.eu>;
    aksw:status "finished";
    aksw:hookline "Social Semantic Collaboration for EKM, E-Learning & E-Tourism";
    dcterms:abstract "Aim of the project is to advance and apply adaptive database and knowledge base techniques as well as user interface techniques to enable large-scale, end-user-driven semantic collaboration in the three application domains Enterprise Knowledge Management, E-Learning and E-Tourism.";
